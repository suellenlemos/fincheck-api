{
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "embeddedLanguageFormatting": "auto",
  "quoteProps": "consistent",
  "jsxBracketSameLine": true,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.ts"]
    }
  ],
  "prettier/prettier": [
    "error",
    {
      "endOfLine": "auto"
    }
  ]
}
